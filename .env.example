APP_ENV=development
POSTGRES_DB=kasperjunge
POSTGRES_USER=kasperjunge
POSTGRES_PASSWORD=replace-with-local-password
POSTGRES_EXPOSE_PORT=5432
DATABASE_URL=postgresql+psycopg://kasperjunge:replace-with-local-password@postgres:5432/kasperjunge
REDIS_EXPOSE_PORT=6379
BACKEND_PORT=8001
SLACK_WEBHOOK_URL=
BACKEND_CORS_ORIGINS=http://localhost:8000
RATE_LIMIT_ENABLED=true
REDIS_URL=redis://redis:6379/0
RATE_LIMIT_CONTACT=10/minute
RATE_LIMIT_ANALYTICS=120/minute
LOG_LEVEL=INFO
LOG_JSON=true
LOG_FILE_PATH=/var/log/kasperjunge/backend.log
LOG_FILE_MAX_BYTES=10485760
LOG_FILE_BACKUP_COUNT=5
